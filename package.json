{
  "name": "react-package-template",
  "version": "1.0.0",
  "description": "Template for building react packages",
  "main": "index.js",
  "scripts": {
    "dist-compile-less": "lessc src/stylesheets/main.less dist/css/main.css",
    "start": "webpack-dev-server",
    "prepublishOnly": "npm run transpile-js; npm run dist-compile-less",
    "transpile-js": "babel src --out-dir dist --ignore src/stylesheets"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ppak10/react-package-template.git"
  },
  "author": "ppak10",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/ppak10/react-package-template/issues"
  },
  "homepage": "https://github.com/ppak10/react-package-template#readme",
  "dependencies": {
    "react": "^16.9.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.5.5",
    "@babel/core": "^7.5.5",
    "@babel/preset-env": "^7.5.5",
    "@babel/preset-react": "^7.0.0",
    "autoprefixer": "^9.6.1",
    "babel-eslint": "^10.0.2",
    "babel-loader": "^8.0.6",
    "css-loader": "^3.2.0",
    "eslint": "^6.1.0",
    "eslint-config-google": "^0.13.0",
    "eslint-config-prettier": "^6.0.0",
    "eslint-plugin-react": "^7.14.3",
    "eslint-plugin-react-hooks": "^1.7.0",
    "html-webpack-plugin": "^3.2.0",
    "husky": "^3.0.4",
    "less": "^3.9.0",
    "less-loader": "^5.0.0",
    "lint-staged": "^9.2.3",
    "postcss-loader": "^3.0.0",
    "precss": "^4.0.0",
    "prettier": "^1.18.2",
    "prop-types": "^15.7.2",
    "react-dom": "^16.9.0",
    "react-router-dom": "^5.0.1",
    "style-loader": "^1.0.0",
    "stylelint": "^10.1.0",
    "stylelint-config-standard": "^18.3.0",
    "stylelint-order": "^3.0.1",
    "webpack": "^4.39.2",
    "webpack-cli": "^3.3.6",
    "webpack-dev-server": "^3.8.0"
  },
  "peerDependencies": {
    "react": "^16.9.0"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  }
}
